<?xml version="1.0" encoding="UTF-8"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xmlns='http://www.ehcache.org/v3'
         xsi:schemaLocation="http://www.ehcache.org/v3 http://www.ehcache.org/schema/ehcache-core-3.0.xsd">

    <cache-template name="simple">
        <expiry>
            <ttl unit="seconds">3600</ttl>
        </expiry>
        <heap>100</heap>
    </cache-template>

    <cache alias="com.softage.epurchase.domain.User" uses-template="simple"/>

    <cache alias="com.softage.epurchase.domain.Authority" uses-template="simple"/>

    <cache alias="com.softage.epurchase.domain.User.authorities" uses-template="simple"/>

    <cache alias="com.softage.epurchase.domain.PersistentToken" uses-template="simple"/>

    <cache alias="com.softage.epurchase.domain.User.persistentTokens" uses-template="simple"/>

    <cache alias="com.softage.epurchase.social.SocialUserConnection" uses-template="simple"/>

    <cache alias="com.softage.epurchase.domain.Employee" uses-template="simple"/>

    <cache alias="com.softage.epurchase.domain.Employee.requisitions" uses-template="simple"/>

    <cache alias="com.softage.epurchase.domain.Department" uses-template="simple"/>

    <cache alias="com.softage.epurchase.domain.Item" uses-template="simple"/>

    <cache alias="com.softage.epurchase.domain.Requisition" uses-template="simple"/>

    <cache alias="com.softage.epurchase.domain.Requisition.items" uses-template="simple"/>

    <cache alias="com.softage.epurchase.domain.Vendor" uses-template="simple"/>

    <cache alias="com.softage.epurchase.domain.Vendor.items" uses-template="simple"/>

    <cache alias="com.softage.epurchase.domain.Item.vendors" uses-template="simple"/>

    <cache alias="com.softage.epurchase.domain.Requisition.requisitions" uses-template="simple"/>

    <cache alias="com.softage.epurchase.domain.ReqItem" uses-template="simple"/>

    <cache alias="com.softage.epurchase.domain.Department.departmentreqs" uses-template="simple"/>

    <cache alias="com.softage.epurchase.domain.Department.departmentords" uses-template="simple"/>

    <cache alias="com.softage.epurchase.domain.PurchaseOrder" uses-template="simple"/>

    <!-- jhipster-needle-ehcache-add-entry -->
</config>
